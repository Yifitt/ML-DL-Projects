        "class LinearRegressionFromScratch:\n",
        "    def __init__(self, learning_rate=0.01, max_iterations=1000, tolerance=1e-6):\n",
        "        self.learning_rate = learning_rate\n",
        "        self.max_iterations = max_iterations\n",
        "        self.tolerance = tolerance\n",
        "        self.weights = None\n",
        "        self.bias = None\n",
        "        self.mse_history = []\n",
        "\n",
        "    def fit(self, X, y):\n",
        "        \"\"\"\n",
        "        X: feature matrix (n_samples, n_features)\n",
        "        y: target vector (n_samples,)\n",
        "        \"\"\"\n",
        "        # turn X into a np array\n",
        "       
        "        X = np.array(X)\n",
        "        y = np.array(y).reshape(-1, 1)\n",
        "\n",
        "        n_samples, n_features = X.shape\n",
        "\n",
        "    \n",
        "        self.weights = np.random.normal(0, 0.01, size=(n_features, 1))\n",
        "        self.bias = 0\n",
        "\n",
        "        # Gradient Descent\n",
        "        prev_mse = float('inf')\n",
        "\n",
        "        for i in range(self.max_iterations):\n",
        "            # Forward pass - prediction\n",
        "            y_pred = self.predict(X, training=True)\n",
        "\n",
        "    \n",
        "            mse = self.mean_squared_error(y, y_pred)\n",
        "            self.mse_history.append(mse)\n",
        "\n",
        "            # Convergence check\n",
        "            if abs(prev_mse - mse) < self.tolerance:\n",
        "                print(f\"Converged at iteration {i}\")\n",
        "                break\n",
        "            prev_mse = mse\n",
        "\n",
        "           \n",
        "            dw = (1/n_samples) * X.T.dot(y_pred - y)\n",
        "            db = (1/n_samples) * np.sum(y_pred - y)\n",
        "\n",
        "          \n",
        "            self.weights -= self.learning_rate * dw\n",
        "            self.bias -= self.learning_rate * db\n",
        "\n",
        "            \n",
        "            if i % 100 == 0:\n",
        "                print(f\"Iteration {i}, MSE: {mse:.6f}\")\n",
        "\n",
        "    def predict(self, X, training=False):\n",
        "        \"\"\"\n",
        "        Prediction\n",
        "        \"\"\"\n",
        "        X = np.array(X)\n",
        "        if training:\n",
        "            return X.dot(self.weights) + self.bias\n",
        "        else:\n",
        "            return (X.dot(self.weights) + self.bias).flatten()\n",
        "\n",
        "    def mean_squared_error(self, y_true, y_pred):\n",
        "        \"\"\"\n",
        "        MSE\n",
        "        \"\"\"\n",
        "        return np.mean((y_true - y_pred) ** 2)\n",
        "\n",
        "    def r2_score(self, y_true, y_pred):\n",
        "        \"\"\"\n",
        "        RÂ² score\n",
        "        \"\"\"\n",
        "        y_true = np.array(y_true)\n",
        "        y_pred = np.array(y_pred)\n",
        "\n",
        "        ss_tot = np.sum((y_true - np.mean(y_true)) ** 2)\n",
        "        ss_res = np.sum((y_true - y_pred) ** 2)\n",
        "\n",
        "        return 1 - (ss_res / ss_tot)\n",
        "\n",
        "    def plot_mse_history(self):\n",
        "        \"\"\"\n",
        "        Plot MSE history\n",
        "        \"\"\"\n",
        "        plt.figure(figsize=(10, 6))\n",
        "        plt.plot(self.mse_history)\n",
        "        plt.title('MSE During Training')\n",
        "        plt.xlabel('Iteration')\n",
        "        plt.ylabel('Mean Squared Error')\n",
        "        plt.grid(True)\n",
        "        plt.show()\n"
      ]
    },
